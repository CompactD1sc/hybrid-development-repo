<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="PictureWrapView">
        <control type="image">
            <left>0</left>
            <top>0</top>
            <width>1280</width>
            <height>45</height>
            <texture flipy="true">floor.png</texture>
            <animation effect="slide" start="0,-10" end="0,0" time="200" condition="Window.Previous(Home)">WindowOpen</animation>
            <animation effect="slide" start="0,0" end="0,-10" time="200" condition="Window.Next(Home)">WindowClose</animation>
            <visible>Control.IsVisible(510)</visible>
        </control>
        <control type="group">
            <visible>Control.IsVisible(510)</visible>
            <include>VisibleFadeEffect</include>
            <left>0</left>
            <top>440</top>
            <control type="wraplist" id="510">
                <left>-80</left>
                <top>5</top>
                <width>1600</width>
                <height>250</height>
                <onleft>510</onleft>
                <onright>510</onright>
                <onup>2</onup>
                <ondown>60</ondown>
                <viewtype label="31645">wrap</viewtype>
                <pagecontrol>60</pagecontrol>
                <scrolltime>200</scrolltime>
                <focusposition>4</focusposition>
                <orientation>horizontal</orientation>
                <preloaditems>4</preloaditems>
                <itemlayout height="200" width="160">
                    <control type="image">
                        <left>10</left>
                        <top>60</top>
                        <width>140</width>
                        <height>140</height>
                        <aspectratio>stretch</aspectratio>
                        <bordertexture border="10">ThumbShadow.png</bordertexture>
                        <bordersize>8</bordersize>
                        <fadetime>200</fadetime>
                        <texture border="2">ThumbBG.png</texture>
                    </control>
                    <control type="image">
                        <left>20</left>
                        <top>70</top>
                        <width>120</width>
                        <height>120</height>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$INFO[ListItem.icon]</texture>
                    </control>
                </itemlayout>
                <focusedlayout height="200" width="160">
                    <control type="image">
                        <left>10</left>
                        <top>60</top>
                        <width>140</width>
                        <height>140</height>
                        <aspectratio>stretch</aspectratio>
                        <bordertexture border="10">ThumbShadow.png</bordertexture>
                        <bordersize>8</bordersize>
                        <fadetime>200</fadetime>
                        <texture border="2">ThumbBG.png</texture>
                        <animation reversible="false" effect="zoom" start="10,60,140,140" end="-15,15,190,190" time="200">focus</animation>
                        <animation reversible="false" effect="zoom" end="10,60,140,140" start="-15,15,190,190" time="200">unfocus</animation>
                    </control>
                    <control type="image">
                        <left>20</left>
                        <top>70</top>
                        <width>120</width>
                        <height>120</height>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$INFO[ListItem.icon]</texture>
                        <animation reversible="false" effect="zoom" start="20,70,120,120" end="0,30,160,160" time="200">focus</animation>
                        <animation reversible="false" effect="zoom" end="20,70,120,120" start="0,30,160,160" time="200">unfocus</animation>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="60">
                <left>310</left>
                <top>200</top>
                <width>660</width>
                <height>25</height>
                <texturesliderbackground border="14,0,14,0">ScrollBarH.png</texturesliderbackground>
                <texturesliderbar border="14,0,14,0">ScrollBarH_bar.png</texturesliderbar>
                <texturesliderbarfocus border="14,0,14,0">ScrollBarH_bar_focus.png</texturesliderbarfocus>
                <textureslidernib>ScrollBarNib.png</textureslidernib>
                <textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
                <onup>510</onup>
                <ondown>800</ondown>
                <showonepage>true</showonepage>
                <orientation>horizontal</orientation>
                <visible>Control.IsVisible(510)</visible>
            </control>
        </control>
    </include>
    <include name="PictureThumbView">
        <control type="group">
            <visible>Control.IsVisible(514)</visible>
            <control type="panel" id="514">
                <left>60</left>
                <top>100</top>
                <width>402</width>
                <height>536</height>
                <onleft>2</onleft>
                <onright>60</onright>
                <onup>514</onup>
                <ondown>514</ondown>
                <viewtype label="$LOCALIZE[31646]">list</viewtype>
                <pagecontrol>60</pagecontrol>
                <scrolltime>200</scrolltime>
                <preloaditems>2</preloaditems>
                <itemlayout height="134" width="134">
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>134</width>
                        <height>134</height>
                        <aspectratio>stretch</aspectratio>
                        <bordertexture border="10">ThumbShadow.png</bordertexture>
                        <bordersize>8</bordersize>
                        <fadetime>200</fadetime>
                        <texture border="2">ThumbBG.png</texture>
                    </control>
                    <control type="image">
                        <left>10</left>
                        <top>10</top>
                        <width>114</width>
                        <height>114</height>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$INFO[ListItem.icon]</texture>
                    </control>
                </itemlayout>
                <focusedlayout height="134" width="134">
                    <control type="image">
                        <left>0</left>
                        <top>0</top>
                        <width>134</width>
                        <height>134</height>
                        <aspectratio>stretch</aspectratio>
                        <bordertexture border="10">folder-Focus.png</bordertexture>
                        <bordersize>8</bordersize>
                        <fadetime>200</fadetime>
                        <texture border="2">ThumbBG.png</texture>
                    </control>
                    <control type="image">
                        <left>10</left>
                        <top>10</top>
                        <width>114</width>
                        <height>114</height>
                        <aspectratio>keep</aspectratio>
                        <texture background="true">$INFO[ListItem.icon]</texture>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="60">
                <left>500</left>
                <top>110</top>
                <width>25</width>
                <height>516</height>
                <texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
                <texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
                <texturesliderbarfocus border="0,14,0,14">ScrollBarV_bar_focus.png</texturesliderbarfocus>
                <textureslidernib>ScrollBarNib.png</textureslidernib>
                <textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
                <onleft>514</onleft>
                <onright>2</onright>
                <showonepage>true</showonepage>
                <orientation>vertical</orientation>
                <visible>Control.IsVisible(514)</visible>
            </control>
            <control type="group">
                <left>570</left>
                <top>80</top>
                <control type="image">
                    <left>0</left>
                    <top>30</top>
                    <width>640</width>
                    <height>470</height>
                    <texture background="true">$INFO[ListItem.FilenameAndPath]</texture>
                    <fadetime>600</fadetime>
                    <aspectratio>keep</aspectratio>
                    <bordertexture border="8">ThumbShadow.png</bordertexture>
                    <bordersize>9</bordersize>
                </control>
                <control type="label">
                    <description>Date time txt</description>
                    <left>0</left>
                    <top>510</top>
                    <width>640</width>
                    <height>15</height>
                    <label>$INFO[ListItem.PicturedateTime,[COLOR=blue]$LOCALIZE[31513][/COLOR] - ]</label>
                    <align>left</align>
                    <aligny>center</aligny>
                    <font>font13</font>
                    <textcolor>white</textcolor>
                    <shadowcolor>black</shadowcolor>
                </control>
                <control type="label">
                    <description>Resolution txt</description>
                    <left>0</left>
                    <top>535</top>
                    <width>640</width>
                    <height>15</height>
                    <label>$INFO[ListItem.PictureResolution,[COLOR=blue]$LOCALIZE[31514][/COLOR] - ]</label>
                    <align>left</align>
                    <aligny>center</aligny>
                    <font>font13</font>
                    <textcolor>white</textcolor>
                    <shadowcolor>black</shadowcolor>
                </control>
            </control>
        </control>
    </include>
</includes>