				<visible>StringCompare(ListItem.Label,..) + [Container.Content(Seasons) | Container.Content(Episodes)]</visible>
			</control>
			<control type="image">
				<include>Dimensions_Fullscreen</include>
				<aspectratio>scale</aspectratio>
