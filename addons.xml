<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="repo.hybrid" name="Hybrid Development Repo" version="1.0.0" provider-name="Mudislander">
  <extension point="xbmc.addon.repository" name="Hybrid Development Repo">
  <info>http://hybrid-development-repo.googlecode.com/svn/trunk/addons.xml</info>
  <checksum>http://hybrid-development-repo.googlecode.com/svn/trunk/addons.xml.md5</checksum>
  <datadir zip="true">http://hybrid-development-repo.googlecode.com/svn/trunk</datadir>
  </extension>
  <extension point="xbmc.addon.metadata">
  <summary>Hybrid Development Repository</summary>
  <description>Repository containing the development version of XBMC's skin.hybrid </description>
  <disclaimer>You break it - You get to keep the pieces</disclaimer>
  <license>GPL</license>
  <platform>all</platform>
  <noicon>false</noicon>
  <nofanart>false</nofanart>
  </extension>
  </addon>
<addon
  id="skin.ccm.helix"
  version="3.0.0"
  name="CCM Helix"
  provider-name="Mudislander">
  <requires>
	<import addon="xbmc.gui" version="5.3.0"/>
	<import addon="script.favourites" version="6.0.1"/>
	<import addon="script.randomandlastitems" version="2.2.0"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    defaultthemename="textures.xbt"
    debugging="false"
    effectslowdown="0.75">
    <res width="1280" height="720" aspect="16:9" default="true" folder="720p" />
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Mudislander's MOD of Confluence skin originally by Jezz_X.</summary>
    <description lang="en">CCM is Mudislander's MOD of Confluence to enable a greater degree of user customisation to the default skin.</description>
    <disclaimer lang="en">You break it - You get to keep the pieces</disclaimer>
    <platform>all</platform>
  </extension>
</addon>
</addons>