	<include name="HomeAddonsCommonLayout">
		<include>Window_OpenClose_Animation</include>
		<animation type="Visible">
			<effect type="slide" start="0,30" end="0,0" delay="200" easing="out" tween="quadratic" time="200" />
			<effect type="fade" start="0" end="100" delay="200" time="200" />
		</animation>
		<animation type="Hidden">
			<!--effect type="zoom" start="100" end="70" center="auto" easing="in" tween="quadratic" time="300" /-->
			<effect type="fade" start="100" end="0" time="200" />
		</animation>
		<hitrect x="0" y="530" w="1280" h="120" />
		<left>65</left>
		<top>190r</top>
		<width>1274</width>
		<height>200</height>
		<pagecontrol>-</pagecontrol>
		<scrolltime>300</scrolltime>
		<orientation>Horizontal</orientation>
		<itemlayout height="180" width="164">
			<control type="image">
				<left>0</left>
				<top>0</top>
				<width>162</width>
				<height>108</height>
				<aspectratio aligny="bottom">keep</aspectratio>
				<bordertexture border="5">button-nofocus.png</bordertexture>
				<bordersize>3</bordersize>
				<texture background="true">$INFO[ListItem.Icon]</texture>
			</control>
			<control type="image">
				<left>5</left>
				<top>108</top>
				<width>152</width>
				<height>100</height>
				<aspectratio aligny="top">keep</aspectratio>
				<texture diffuse="diffuse_mirror2.png" flipy="true" background="true">$INFO[ListItem.Icon]</texture>
			</control>
			<control type="label">
				<left>1</left>
				<top>110</top>
				<width>162</width>
				<height>20</height>
				<font>font12</font>
				<textcolor>grey2</textcolor>
				<align>center</align>
				<aligny>center</aligny>
				<label>$INFO[ListItem.Label]</label>
			</control>
		</itemlayout>
		<focusedlayout height="180" width="164">
			<control type="image">
				<left>0</left>
				<top>0</top>
				<width>162</width>
				<height>108</height>
				<aspectratio aligny="bottom">keep</aspectratio>
				<bordertexture border="5">button-nofocus.png</bordertexture>
				<bordersize>3</bordersize>
				<texture background="true">$INFO[ListItem.Icon]</texture>
				<visible>!ControlGroup(9002).HasFocus</visible>
			</control>
			<control type="image">
