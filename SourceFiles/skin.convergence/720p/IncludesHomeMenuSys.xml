<?xml version="1.0" encoding="utf-8"?>
<includes>
    <include name="HomeVerticalMainMenu">
        <control type="fixedlist" id="9000">
            <description>Home Wraplist</description>
            <hitrect x="5" y="5" w="330" h="650" />
            <left>-20</left>
            <top>35</top>
            <width>400</width>
            <height>600</height>
            <onup>9000</onup>
            <ondown>9000</ondown>
            <onleft condition="Player.HasMedia">20</onleft>
            <onleft condition="!Player.HasMedia">22</onleft>
            <onright>9001</onright>
            <pagecontrol>-</pagecontrol>
            <scrolltime>550</scrolltime>
            <focusposition>2</focusposition>
            <movement>2</movement>
            <animation effect="fade" start="0" end="100" time="200">Visible</animation>
            <animation effect="fade" start="100" end="30" time="200" condition="ControlGroup(9001).HasFocus | Control.HasFocus(9002)">conditional</animation>
            <itemlayout height="102" width="380">
                <control type="label">
                    <left>0</left>
                    <top>21</top>
                    <width>380</width>
                    <height>40</height>
                    <font>font_MainMenu</font>
                    <textcolor>MenuNoFocus</textcolor>
                    <align>right</align>
                    <aligny>center</aligny>
                    <label>$INFO[ListItem.Label]</label>
                    <animation effect="zoom" start="65" end="65" center="380,51" time="0" condition="true">Conditional</animation>
                    <animation effect="fade" start="100" end="30" time="200" condition="ControlGroup(9001).HasFocus">conditional</animation>
                </control>
            </itemlayout>
            <focusedlayout height="102" width="380">
                <!--include>HomeSubMenuInfo</include-->
                <include>HomeHasSub</include>
                <control type="label">
                    <left>0</left>
                    <top>0</top>
                    <width>380</width>
                    <height>102</height>
                    <font>font_MainMenu</font>
                    <textcolor>MenuFocus</textcolor>
                    <align>right</align>
                    <aligny>center</aligny>
                    <animation effect="zoom" start="65" end="110" center="380,51" time="200">Focus</animation>
                    <animation effect="zoom" start="110" end="65" center="380,51" time="200">UnFocus</animation>
                    <label>$INFO[ListItem.Label]</label>
                </control>
            </focusedlayout>
            <include>HomeMainMenuContent</include>
            <visible>!Skin.HasSetting(HorizontalHomeScreen)</visible>
        </control>
    </include>
    <include name="HorizontalWrapMenu">
        <control type="wraplist" id="9000">
            <include>HomeMainMenuSys</include>
            <include>HomeMainMenuContent</include>
            <visible>Skin.HasSetting(WrapAroundHomeMenu)</visible>
            <visible>Skin.HasSetting(HorizontalHomeScreen)</visible>
        </control>
    </include>
    <include name="HorizontalFixedMenu">
        <control type="fixedlist" id="9000">
            <include>HomeMainMenuSys</include>
            <include>HomeMainMenuContent</include>
            <visible>!Skin.HasSetting(WrapAroundHomeMenu)</visible>
            <visible>Skin.HasSetting(HorizontalHomeScreen)</visible>
        </control>
    </include>
    <include name="HomeMainMenuSys">
        <left>-110</left>
        <top>5</top>
        <width>1500</width>
        <height>60</height>
        <onleft condition="!Skin.HasSetting(FixedEndingMenu)">9000</onleft>
        <onright condition="!Skin.HasSetting(FixedEndingMenu)">9000</onright>
        <onleft condition="Skin.HasSetting(FixedEndingMenu)">noop</onleft>
        <onright condition="Skin.HasSetting(FixedEndingMenu)">noop</onright>
        <onup>9003</onup>
        <ondown>9001</ondown>
        <!--onup condition="!Skin.HasSetting(DisableGlobalSearch)">608</onup-->
        <!--onup condition="Skin.HasSetting(DisableGlobalSearch)">603</onup-->
        <!--ondown>9001</ondown-->
        <pagecontrol>-</pagecontrol>
        <scrolltime>300</scrolltime>
        <focusposition>2</focusposition>
        <movement>1</movement>
        <orientation>Horizontal</orientation>
        <itemlayout height="60" width="300">
            <include>HomeSeperator</include>
            <control type="label">
                <left>5</left>
                <top>0</top>
                <width>290</width>
                <height>60</height>
                <font>font_HorzMainMenu</font>
                <textcolor>MenuNoFocus</textcolor>
                <align>center</align>
                <aligny>center</aligny>
                <label>$INFO[ListItem.Label]</label>
            </control>
        </itemlayout>
        <focusedlayout height="60" width="300">
            <include>HomeSeperator</include>
            <include>HomeHasSub</include>
            <control type="label">
                <left>5</left>
                <top>0</top>
                <width>290</width>
                <height>60</height>
                <font>font_HorzMainMenu</font>
                <textcolor>MenuNoFocus</textcolor>
                <align>center</align>
                <aligny>center</aligny>
                <label>$INFO[ListItem.Label]</label>
                <visible>!Control.HasFocus(9000)</visible>
            </control>
            <control type="label">
                <left>5</left>
                <top>0</top>
                <width>290</width>
                <height>60</height>
                <font>font_HorzMainMenu</font>
                <textcolor>MenuFocus</textcolor>
                <align>center</align>
                <aligny>center</aligny>
                <label>$INFO[ListItem.Label]</label>
                <visible>Control.HasFocus(9000)</visible>
            </control>
        </focusedlayout>
        <visible>Skin.HasSetting(HorizontalHomeScreen)</visible>
    </include>
