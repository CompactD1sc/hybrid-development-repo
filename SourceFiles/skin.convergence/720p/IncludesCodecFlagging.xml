			<width>55</width>
			<height>35</height>
			<aspectratio align="left">keep</aspectratio>
			<texture>$INFO[ListItem.AudioChannels,flagging/audio/,.png]</texture>
		</control>
	</include>
	<include name="AspectCodecFlaggingConditions">
